{
  "name": "mini-md",
  "version": "2.0.6",
  "description": "An express view engine that serves as a templating engine for markdown files",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "frodi-karlsson <frodikarlsson@gmail.com>",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "express",
    "markdown",
    "templating",
    "markdown-it"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/frodi-karlsson/mini-md"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build",
    "lint": "eslint ."
  },
  "dependencies": {
    "@naturalcycles/js-lib": "^14.225.0",
    "@naturalcycles/nodejs-lib": "^13.14.0",
    "express": "^4.19.2",
    "markdown-it": "^14.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/markdown-it": "^14.0.0",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.56",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.6.0"
  }
}
